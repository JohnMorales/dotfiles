# John Morales <jmorales@gmail.com> tigrc
# option references for this file: http://jonas.nitro.dk/tig/tigrc.5.html
set refresh-mode = auto
set show-changes = true
set line-graphics = utf-8
set main-view = date:default author id commit-title:graph=true,refs=true
bind log J :/^commit
bind log K :?^commit
